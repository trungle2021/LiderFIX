	padding: 0 0
}

fieldset {
	border: 0
}

body {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%
}

#overlay {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #111;
	opacity: 0.7;
