	.switch-language{
    position: relative !important;
		display: inline-block;
    margin: 0 5px;
  }
  
  .switch-language > span{
    position: absolute;
    top: 6px;
    pointer-events: none;
    font-family: "Helvetica", Arial, sans-serif;
    font-weight: bold;
    font-size: 9px;
    text-transform: uppercase;
    /* text-shadow: 0 1px 0 rgba(0, 0, 0, 0.06); */
    width: 50%;
    text-align: center;
  }


  
  .switch-language > span.on{
    left: 0;
    padding-left: 2px;
    color: #fff;
  }
  
  .switch-language > span.off {
    right: 0;
    padding-right: 4px;
    color: #333333;
  }
  
  .check-toggle {
    position: absolute;
    margin-left: -9999px;
    visibility: hidden;
  }
  
  .check-toggle + label {
    display: block;
    position: relative;
    cursor: pointer;
    outline: none;
    user-select: none;
  }
  
  input.check-toggle-round-flat:checked ~ .off {
    color: #fff;
  }
  
  input.check-toggle-round-flat:checked ~ .on {
    color: #333333;
  }
  
  input.check-toggle-round-flat + label {
    padding: 2px;
    width: 100px;
    height: 22px;
    background-color: #dad8c9;
    border-radius: 60px;
  }
  
  input.check-toggle-round-flat + label:before,
  input.check-toggle-round-flat + label:after {
    display: block;
    position: absolute;
    content: "";
  }
  
  input.check-toggle-round-flat + label:before {
    top: 2px;
    left: 2px;
    bottom: 2px;
    right: 2px;
    background-color: #dad8c9;
    border-radius: 60px;
  }
  
  input.check-toggle-round-flat + label:after {
    top: 4px;
    right: 4px;
    bottom: 4px;
    width: 48px;
    background-color: #fff;
    border-radius: 52px;
    transition: margin 0.2s;
  }
  
  input.check-toggle-round-flat:checked + label:after {
    margin-right: 44px;
  }