		padding: 0 20px 15px 20px;
}

.switch-language-mobile > span {
position: absolute;
top: 6px;
pointer-events: none;
font-family: "Helvetica", Arial, sans-serif;
font-weight: bold;
font-size: 9px;
text-transform: uppercase;
width: 63%;
text-align: center;
}


.switch-language-mobile > span.on {
left: 0;
padding-left: 2px;
color: #fff;
}

.switch-language-mobile > span.off {
right: 0;
padding-right: 4px;
color: #333333;
}

.switch-language-mobile .check-toggle-mobile {
position: absolute;
margin-left: -9999px;
visibility: hidden;
}

.switch-language-mobile .check-toggle-mobile + label {
display: block;
position: relative;
cursor: pointer;
outline: none;
user-select: none;
}

.switch-language-mobile input.check-toggle-round-flat-mobile:checked ~ .off {
color: #fff;
}

.switch-language-mobile input.check-toggle-round-flat-mobile:checked ~ .on {
color: #333333;
}



